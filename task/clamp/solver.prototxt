net: "task/clamp/train_val.prototxt"
test_iter: 8
test_interval: 50
test_compute_loss: 1
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.01
lr_policy: "step"
gamma: 0.9
# stepsize should also be lower, as we're closer to being done
stepsize: 200
display: 1
max_iter: 20000
momentum: 0.9
weight_decay: 0.0005
snapshot: 2000
snapshot_prefix: "task/clamp/fc7"
solver_mode: GPU
