net: "models/clamp/train_val.prototxt"
test_iter: 8
test_interval: 20
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
# stepsize should also be lower, as we're closer to being done
stepsize: 20000
display: 1
max_iter: 100000
momentum: 0.9
weight_decay: 0.0005
snapshot: 20000
snapshot_prefix: "task/clamp/fc7/clamp_fc7"
solver_mode: GPU
